<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="-2dp"
    android:paddingTop="-11dp"
    android:background="@drawable/zadanie_fon_inform">

    <RelativeLayout
        android:id="@+id/layoutinf"
        android:layout_width="match_parent"
        android:layout_height="425dp"
        app:layout_constraintBottom_toTopOf="@+id/algCheck"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout6">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                app:srcCompat="@drawable/infphoto1_1" />

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_gravity="right"
                app:srcCompat="@drawable/algtask1right" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_gravity="right"
                app:srcCompat="@drawable/infphoto1_2" />

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_gravity="right"
                app:srcCompat="@drawable/algtask1right" />

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_gravity="right"
                app:srcCompat="@drawable/infphoto1_3" />

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_gravity="right"
                app:srcCompat="@drawable/algtask1right" />
        </LinearLayout>

        <TextView
            android:id="@+id/textView8"
            android:layout_width="160dp"
            android:layout_height="45dp"
            android:background="@drawable/podkltaskpole"
            android:text="пересечение"
            android:textColor="#3D0250"
            android:textSize="26sp" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="160dp"
            android:layout_height="45dp"
            android:background="@drawable/podkltaskpole"
            android:text="соподчинение"
            android:textColor="#3D0250"
            android:textSize="26sp" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="160dp"
            android:layout_height="45dp"
            android:textColor="#3D0250"
            android:background="@drawable/podkltaskpole"
            android:text="подчинение "
            android:textSize="26sp" />

    </RelativeLayout>

    <LinearLayout
        android:id="@+id/linearLayout6"
        android:layout_width="408dp"
        android:layout_height="115dp"
        android:layout_marginBottom="424dp"
        android:background="@drawable/podklpereetyag"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/algCheck"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView12">

        <TextView
            android:id="@+id/textView28"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="130dp"
            android:text="Сопоставь "
            android:textColor="#3D0250"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/textView29"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="60dp"
            android:text="определение и рисунок"
            android:textColor="#3D0250"
            android:textSize="24sp" />


    </LinearLayout>

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="match_parent"
        android:layout_height="93dp"
        android:padding="-1dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/podl_inform" />

    <Button
        android:id="@+id/bottomTeoryK1"
        android:layout_width="76dp"
        android:layout_height="62dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/_16"
        app:backgroundTint="#FFFFFF"
        app:iconTint="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="263dp"
        android:layout_height="63dp"
        android:layout_marginTop="16dp"
        android:text="ОСНОВЫ ИНФОРМАТИКИ"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imageView12"
        app:layout_constraintStart_toEndOf="@+id/bottomTeoryK1"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/algCheck"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="48dp"
        android:text="ПРОДОЛЖИТЬ"
        android:textSize="24sp"
        app:backgroundTint="#173602"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
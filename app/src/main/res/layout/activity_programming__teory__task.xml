<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/algoritm_fon"
    tools:context=".Algorithm_Teory_Task">

    <Button
        android:id="@+id/CanleK"
        android:layout_width="76dp"
        android:layout_height="62dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/_16"
        app:backgroundTint="#660000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/HomeBK1"
        android:layout_width="157dp"
        android:layout_height="157dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:background="#00FFFFFF"
        android:drawableBottom="@drawable/homek"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cifraK1" />

    <Button
        android:id="@+id/HomeBK2"
        android:layout_width="170dp"
        android:layout_height="157dp"
        android:background="#00FFFFFF"
        android:drawableBottom="@drawable/homek"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/HomeBK1"
        app:layout_constraintTop_toBottomOf="@+id/cifraK2" />

    <Button
        android:id="@+id/HomeBK3"
        android:layout_width="157dp"
        android:layout_height="157dp"
        android:background="#00FFFFFF"
        android:drawableBottom="@drawable/homek"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cifraK3" />

    <ImageView
        android:id="@+id/cifraK1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="92dp"
        android:layout_marginLeft="92dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/CanleK"
        app:srcCompat="@drawable/_" />

    <ImageView
        android:id="@+id/cifraK2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.631"
        app:layout_constraintStart_toEndOf="@+id/cifraK1"
        app:layout_constraintTop_toBottomOf="@+id/CanleK"
        app:srcCompat="@drawable/_2" />

    <ImageView
        android:id="@+id/cifraK3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/HomeBK1"
        app:srcCompat="@drawable/_3" />

    <ImageView
        android:id="@+id/HomeKK1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cifraK4"
        app:layout_constraintVertical_bias="0.111"
        app:srcCompat="@drawable/homek" />

    <Button
        android:id="@+id/CloseK1"
        android:layout_width="134dp"
        android:layout_height="107dp"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="16dp"
        android:background="#00FFFFFF"
        android:drawableBottom="@drawable/_21"
        app:layout_constraintEnd_toEndOf="@+id/HomeKK1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/HomeKK1" />

    <ImageView
        android:id="@+id/cifraK4"
        android:layout_width="29dp"
        android:layout_height="40dp"
        android:layout_marginStart="92dp"
        android:layout_marginLeft="92dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/HomeBK3"

        app:srcCompat="@drawable/_4" />

    <ImageView
        android:id="@+id/HomeKK2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.535"
        app:layout_constraintStart_toEndOf="@+id/HomeKK1"
        app:layout_constraintTop_toBottomOf="@+id/cifraK5"
        app:srcCompat="@drawable/homek" />

    <Button
        android:id="@+id/CloseK2"
        android:layout_width="136dp"
        android:layout_height="108dp"
        android:background="#00FFFFFF"
        android:drawableBottom="@drawable/_21"
        app:layout_constraintBottom_toBottomOf="@+id/HomeKK2"
        app:layout_constraintEnd_toEndOf="@+id/HomeKK2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/HomeKK2"
        app:layout_constraintTop_toTopOf="@+id/HomeKK2"
        app:layout_constraintVertical_bias="0.638" />

    <ImageView
        android:id="@+id/cifraK5"
        android:layout_width="29dp"
        android:layout_height="40dp"
        android:layout_marginEnd="96dp"
        android:layout_marginRight="96dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.963"
        app:layout_constraintStart_toEndOf="@+id/cifraK4"
        app:layout_constraintTop_toBottomOf="@+id/HomeBK3"
        app:srcCompat="@drawable/_5" />


</androidx.constraintlayout.widget.ConstraintLayout>